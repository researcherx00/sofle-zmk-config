#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";
// -------------------------------------------------------------------------------------------------------------
// |  ESC  |  1  |  2  |  3   |  4   |  5    |                     |  6    |  7    |  8    |  9   |   0   |   \   |
// |  TAB  |  Q  |  W  |  E   |  R   |  T    |                     |  Y    |  U    |  I    |  O   |   P   |   -   |
// |  CAPS |  A  |  S  |  D   |  F   |  G    |                     |  H    |  J    |  K    |  L   |   ;   |   '   |
// | SHIFT |  Z  |  X  |  C   |  V   |  B    |  MUTE |    | RGBEFR |  N    |  M    |  ,    |  .   |   /   | SHIFT |
//               | CTRL | GUI | ALT  | SPC   | TOG 1 |    | RGBTOG | SPACE | ENTER |       | GUI  |
        default_layer {
            bindings = <
&kp ESC     &kp N1    &kp N2        &kp N3     &kp N4  &kp N5                     &kp N6           &kp N7     &kp N8      &kp N9     &kp N0   &kp BACKSPACE
&kp TAB     &kp Q     &kp W         &kp E      &kp R   &kp T                      &kp Y            &kp U      &kp I       &kp O      &kp P   &kp BACKSLASH
&kp CAPS    &kp A     &kp S         &kp D      &kp F   &kp G                      &kp H            &kp J      &kp K       &kp L   &kp SEMI  &kp APOSTROPHE
&kp LSHIFT  &kp Z     &kp X         &kp C      &kp V   &kp B  &kp C_MUTE  &rgb_ug RGB_EFR   &kp N  &kp M  &kp COMMA  &kp PERIOD  &kp SLASH       &kp ENTER
                &kp LCTRL  &kp LWIN  &kp LEFT_ALT  &kp SPACE  &to 1      &rgb_ug RGB_TOG  &kp SPACE    &kp LBKT   &kp RBKT   &kp DEL

            >;
            sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN &inc_dec_kp PG_UP PG_DN>;

        };

        layer_layer_Layer__1 {
// TODO: Some binds are waiting for shifted keycode support.
// ------------------------------------------------------------------------------------------------------------
// |      |  F1 |  F2 |  F3  |  F4  |  F5  |                   |  F6  |  F7   |  F8   |  F9  |  F10  |  F11  |
// |      |     |     |      |      |      |                   | PGUP |   7   |   8   |   9  |       |       |
// |      |  =  |  -  |  +   |      |      |                   | PGDN |   4   |   5   |   6  |   [   |   ]   |
// |      |     |     |      |      |      |        |  |       |      |   1   |   2   |   3  |       |       |
//              |     |      |      |      |        |  |       | CTRL |       |   0   |      |
            bindings = <
&trans   &kp F1    &kp F2    &kp F3      &kp F4    &kp F5                    &kp F6     &kp F7   &kp F8   &kp F9    &kp F10   &kp F11
&trans   &trans    &trans    &trans      &trans    &trans                    &kp PG_UP  &kp N7   &kp N8   &kp N9    &trans    &trans
&trans   &kp EQUAL &kp MINUS &kp KP_PLUS &trans    &trans                    &kp PG_DN  &kp N4   &kp N5   &kp N6    &kp LBKT  &kp RBKT
&trans   &trans    &trans    &trans      &trans    &trans    &trans   &trans &trans     &kp N1   &kp N2   &kp N3    &trans    &trans
                   &trans    &trans      &trans    &trans    &trans   &trans &kp LCTRL  &trans   &kp N0   &trans
            >;

            sensor-bindings = <&inc_dec_kp PG_UP PG_DN &inc_dec_kp C_VOL_UP C_VOL_DN>;
        };

        layer_layer_Layer__2  {
// ------------------------------------------------------------------------------------------------------------
// |BTCLR| BT1  | BT2  |  BT3  |  BT4  |  BT5 |                |      |      |       |      |       |       |
// |     | INS  | PSCR | GUI   |       |      |                |      |      |       |      |       |       |
// |     | ALT  | CTRL | SHIFT |       | CAPS |                |  <-  |   v  |   ^   |  ->  |  DEL  | BKSPC |
// |     | UNDO | CUT  | COPY  | PASTE |      |      |  |      |      |      |       |      |       |       |
//              |      |       |       |      |      |  |      |      |      |       |      |
            bindings = <
&bt BT_CLR &bt BT_SEL 0 &bt BT_SEL 1 &bt BT_SEL 2 &bt BT_SEL 3 &bt BT_SEL 4             &trans    &trans    &trans   &trans    &trans  &trans
&trans     &kp INS      &kp PSCRN    &kp K_CMENU  &trans       &trans                   &trans    &trans    &trans   &trans    &kp N0  &trans
&trans     &kp LALT     &kp LCTRL    &kp LSHFT    &trans       &kp CLCK                 &kp LEFT  &kp DOWN  &kp UP &kp RIGHT &kp DEL &kp BSPC
&trans     &kp K_UNDO   &kp K_CUT    &kp K_COPY   &kp K_PASTE  &trans  &trans   &trans  &trans    &trans    &trans   &trans    &trans  &trans
                        &trans       &trans       &trans       &trans  &trans   &trans  &trans    &trans    &trans   &trans
            >;

            sensor-bindings = <&inc_dec_kp PG_UP PG_DN &inc_dec_kp C_VOL_UP C_VOL_DN>;
        };
    };
};